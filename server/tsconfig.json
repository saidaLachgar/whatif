{
  "compilerOptions": {
    
    /* project */
    "sourceMap": true,
    "outDir": "./dist",
    
    
    /* Language and Environment */
    "target": "es2016",
    
    
    /* Modules */
    "module": "commonjs",
    
    
    /* JavaScript Support */
    
    
    /* Emit */
    
    
    /* Interop Constraints */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    
    
    /* Type Checking */
    "strict": true,
    
    
    /* Completeness */
    "skipLibCheck": true
  }
}